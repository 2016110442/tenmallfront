<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.wanxi.dao.user.UserDao">
    <update id="updatePassword">
        update
        wx_tab_user
        set
        password = #{password}
        where phone = #{phone}
    </update>
</mapper>
